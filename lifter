#!/usr/bin/env python
# -*- indent-tabs-mode: t -*-

import sys
from mapupdater import controller

lambda_map = [[]]
line = sys.stdin.read()
while line != "":
	print line
	for c in line:
		if c == '\n':
			lambda_map.insert(0,[])
			continue
		lambda_map[0].append(c)
	line = sys.stdin.read()

lambda_map.pop(0)


cont = controller(lambda_map)
#example :
cont.set_movement('R')
print lambda_map


